    <script>
        // Page navigation
        document.addEventListener('DOMContentLoaded', function() {
            const navLinks = document.querySelectorAll('.nav-link');
            const pages = document.querySelectorAll('.page');

            navLinks.forEach(link => {
                link.addEventListener('click', function(e) {
                    e.preventDefault();

                    const targetPage = this.getAttribute('data-page');

                    // Update active navigation link
                    navLinks.forEach(navLink => navLink.classList.remove('active'));
                    this.classList.add('active');

                    // Show target page, hide others
                    pages.forEach(page => {
                        page.classList.remove('active');
                        if (page.id === targetPage) {
                            page.classList.add('active');
                        }
                    });
                });
            });

            // Animation for elements
            const observerOptions = {
                threshold: 0.1,
                rootMargin: '0px 0px -50px 0px'
            };

            const observer = new IntersectionObserver((entries) => {
                entries.forEach(entry => {
                    if (entry.isIntersecting) {
                        entry.target.style.opacity = 1;
                        entry.target.style.transform = 'translateY(0)';
                    }
                });
            }, observerOptions);

            // Animate cards on scroll
            const cards = document.querySelectorAll('.tool-card, .resource-card, .stat-card');
            cards.forEach(card => {
                card.style.opacity = 0;
                card.style.transform = 'translateY(20px)';
                card.style.transition = 'opacity 0.5s ease, transform 0.5s ease';
                observer.observe(card);
            });
        });

       function checkHarassment() {
    const message = document.getElementById('message').value.trim();
    const resultDiv = document.getElementById('harassment-result');

    if (!message) {
        resultDiv.innerHTML = '<p>Please enter a message to analyze.</p>';
        return;
    }

    resultDiv.innerHTML = '<p>Analyzing message...</p>';

    // Simple keyword-based mock detection
    const abusiveKeywords = ['stupid', 'idiot', 'hate', 'kill', 'shut up', 'dumb', 'ugly', 'bitch', 'fool'];
    const lowerMessage = message.toLowerCase();

    let foundKeywords = abusiveKeywords.filter(word => lowerMessage.includes(word));

    setTimeout(() => {
        if (foundKeywords.length > 0) {
            resultDiv.innerHTML = `
                <p><strong>Risk Level:</strong> <span class="risk-high">High</span></p>
                <p><strong>Detected abusive words:</strong> ${foundKeywords.join(', ')}</p>
                <p><strong>Advice:</strong> Consider blocking the sender and reporting the message.</p>
            `;
        } else {
            resultDiv.innerHTML = `
                <p><strong>Risk Level:</strong> <span class="risk-low">Low</span></p>
                <p>No abusive language detected.</p>
                <p>Stay safe and vigilant.</p>
            `;
        }
    }, 1000);
}

        function checkPrivacy() {
            const username = document.getElementById('username').value;
            const resultDiv = document.getElementById('privacy-result');

            if (!username.trim()) {
                resultDiv.innerHTML = '<p>Please enter a username to check.</p>';
                return;
            }

            // Simulate privacy check results
            const randomScore = Math.floor(Math.random() * 100);
            let privacyLevel = 'Good';
            let color = '#2ecc71';

            if (randomScore < 30) {
                privacyLevel = 'Poor';
                color = '#e74c3c';
            } else if (randomScore < 70) {
                privacyLevel = 'Fair';
                color = '#f39c12';
            }

            resultDiv.innerHTML = `
                <p><strong>Username:</strong> ${username}</p>
                <p><strong>Privacy Score:</strong> <span style="color: ${color}">${randomScore}/100 (${privacyLevel})</span></p>
                <p><strong>Recommendations:</strong> Review your social media privacy settings.</p>
            `;
        }
    </script>
</body>
</html>
"""

display(HTML(html_content))
